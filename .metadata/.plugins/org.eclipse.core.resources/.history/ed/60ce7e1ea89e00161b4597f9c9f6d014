#include <iostream>
#include "list.h"
#include <string>
#include <sstream>
using namespace std;

/*
 * ----------------------测试专用区------------------------------
 */
void test() {
	LinkedList *list = new LinkedList();
	list->add(1);
	list->add(2);
	list->add(3);
	list->add(4);
	list->add(5);

	list->insert(6,5);
	list->remove(6);
	list->rotate(2);
	cout << list->getSize()<<endl;
	list->print();
	delete list;
}
/*
 * -------------------------------------------------------------------
 */
bool printBefore = false;
void parseInput(LinkedList *, int[]);

int main() {
	LinkedList *list = new LinkedList();
	while(true) {
		int num;
		cin >> num;
		if(num == -1) {
			break;
		}
		list->add(num);
	}
	int count;
	cin >> count;

	stringstream ss;
	int operation[count][3];
	for(int i = 1; i <= count ; i ++) {
		string line ;
		getline(cin, line);
		ss << line;
		int operation[3];
		int k = 0;
		while(ss >> operation[k ++]);

	}
}

void parseInput(LinkedList *list , int operation[]) {
	switch(operation[0]) {
	case 0:
		list->print();
		if(printBefore) {
			cout<< endl;
		} else {
			printBefore = true;
		}
		break;
	case 1:
		list->insert(operation[1], operation[2]);
		break;
	case 2:
		list->remove(operation[1]);
		break;
	case 3:
		list->rotate(operation[1]);
		break;
	default:
		break;
	}

}
