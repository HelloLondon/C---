#include <iostream>
using namespace std;


int main() {
	const int c = 128;
	int *q = const_cast<int *>(&c);
	*q = 111;
	cout << "c" << &c << c << endl;
	cout << "q" << &q << q << endl;
	cout << "*q" << q << *q << endl;
}


