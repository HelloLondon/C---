#include <iostream>
using namespace std;

bool checkTheSame(int[], int);

int main(){
	int n;
	cin >> n;
	int datas[5];
	for (int i = 0 ; i < n ; i ++) {
		cin >> datas[i];
	}
	cout << checkTheSame(datas, n-1);
}

bool checkTheSame(int[] datas, int end) {
	for (int i = 0; i <= end; i ++) {
		for (int j = i; j >= 0; j ++) {
			if(datas[i] == datas[j]) {
				return true;
			}
		}
	}
	return false;
}
