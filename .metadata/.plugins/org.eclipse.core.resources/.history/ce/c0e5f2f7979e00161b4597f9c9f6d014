#define NULL 0

struct ListNode {
	int val;
	ListNode *next;
	ListNode(int x) :valx),next(NULL){}
};

class LinkedList {

private:
	ListNode *start;
	ListNode *last;
	int size;
public:
	LinkedList();
	~LinkedList();
	void add(int x);
	int get();
	 void remove(int value);
	 void insert(int val, int pos);
	 void print();
};
