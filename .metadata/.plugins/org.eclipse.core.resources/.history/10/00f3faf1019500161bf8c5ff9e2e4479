#include <iostream>
using namespace std;

int main() {
	int n ;
	cin >> n;
	double data[n];
	for (int i = 0; i < n ; i ++) {
		cin >> data[i];
	}
	double maxP = data[0];
	double cur = 1;
	for (int i = 0; i < n; i ++) {
		cur = 1;
		for (int j = i; j < n; j ++) {
			cur *= data[j];
			if (cur > maxP) {
				maxP = cur;
			}

		}
	}
	cout << maxP;
}
