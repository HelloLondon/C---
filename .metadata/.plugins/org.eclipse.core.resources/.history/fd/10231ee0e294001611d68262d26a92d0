#include <iostream>
#include <string>
#include <cstring>
using namespace std;

struct TStudent {
	char name[20];
	int age;
};

int compareByName (const void*elem1,const void*elem2) {

	strcmp(((TStudent *)elem1)->name,((TStudent *)elem2)->name);
}

int main() {
	const int c = 128;
	int *q = const_cast<int *>(&c);
	*q = 111;
	cout << "c" << &c << c << endl;
	cout << "q" << &q << q << endl;
	cout << "*q" << q << *q << endl;
	return 0;
}




